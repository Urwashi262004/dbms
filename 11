studentdb> db.student.find().limit(3); 
[ 
  { _id: ObjectId("..."), name: 'Shruti', age: 19, city: 'Pune' }, 
  { _id: ObjectId("..."), name: 'Harsh', age: 20, city: 'Nagpur' }, 
  { _id: ObjectId("..."), name: 'Jash', age: 18, city: 'Sangli' } 
] 
 
studentdb> db.student.find({ age: { $gt: 19 } }, { name: 1, age: 1 }); 
[ 
  { _id: ObjectId("..."), name: 'Harsh', age: 20 }, 
  { _id: ObjectId("..."), name: 'Suyash', age: 20 } 
] 
 
studentdb> db.student.findOne({ name: "Jash" }); 
{ _id: ObjectId("..."), name: 'Jash', age: 18, city: 'Sangli' } 
 
studentdb> db.student.updateOne( 
  { name: "Sahil" }, 
  { $set: { email: "sahil@gmail.com" } } 
); 
{ 
  acknowledged: true,   insertedId: null,   matchedCount: 1,   modifiedCount: 1,   upsertedCount: 0 
} 
 
studentdb> db.student.findOne({ name: "Sahil" }); 
{ _id: ObjectId("..."), name: 'Sahil', age: 19, city: 'Kolhapur', email: 'sahil@gmail.com' } 
 
studentdb> db.student.updateMany( 
  { age: { $gt: 19 } }, 
  { $set: { city: "Mumbai" } } 
); 
{ 
  acknowledged: true,   insertedId: null,   matchedCount: 2,   modifiedCount: 2,   upsertedCount: 0 
} 
 
