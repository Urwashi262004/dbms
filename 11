studentdb> db.student.deleteMany({ age: 20 }); 
{ acknowledged: true, deletedCount: 2 } 
 
studentdb> db.student.find(); 
[ 
  { _id: ObjectId("..."), name: 'Jash', age: 18, city: 'Sangli' }, 
  { _id: ObjectId("..."), name: 'Sahil', age: 19, city: 'Kolhapur', email: 'sahil@gmail.com' } 
] 
 
studentdb> db.student.find({ age: { $lte: 18 } }); 
[ 
  { _id: ObjectId("..."), name: 'Jash', age: 18, city: 'Sangli' } 
] 
 
studentdb> db.student.find({ age: { $gte: 19 } }); 
[ 
  { _id: ObjectId("..."), name: 'Sahil', age: 19, city: 'Kolhapur', email: 'sahil@gmail.com' } 
] 
 
studentdb> db.student.find({ age: { $in: [18, 19, 20] } }); 
[ 
  { _id: ObjectId("..."), name: 'Jash', age: 18, city: 'Sangli' }, 
  { _id: ObjectId("..."), name: 'Sahil', age: 19, city: 'Kolhapur', email: 'sahil@gmail.com' } 
] 
 
studentdb> db.student.find({ age: { $nin: [19, 20] } }); 
[ 
  { _id: ObjectId("..."), name: 'Jash', age: 18, city: 'Sangli' } 
] 
 
 
 
